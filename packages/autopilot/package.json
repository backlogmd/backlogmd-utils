{
  "name": "@backlogmd/autopilot",
  "version": "0.1.0",
  "description": "LangChain.js agent for BacklogMD that uses the serve /chat endpoint",
  "type": "module",
  "license": "MIT",
  "author": "BacklogMD",
  "repository": {
    "type": "git",
    "url": "https://github.com/backlogmd/backlogmd-utils.git",
    "directory": "packages/autopilot"
  },
  "keywords": ["backlogmd", "autopilot", "langchain", "agent", "chat"],
  "publishConfig": { "access": "public" },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": { "types": "./dist/index.d.ts", "import": "./dist/index.js" },
    "./package.json": "./package.json"
  },
  "files": ["dist", "skills"],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'"
  },
  "dependencies": {
    "@backlogmd/core": "*",
    "@langchain/classic": "^1.0.0",
    "@langchain/core": "^1.1.0",
    "langchain": "^1.2.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": { "node": ">=22" }
}
